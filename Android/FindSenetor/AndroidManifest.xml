        <activity
            android:name="com.swapp.findsenetor.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.swapp.findsenetor.SearchPage"
            android:label="search_page" >
        </activity>
        <activity
            android:name="com.swapp.findsenetor.List_Profiles"
            android:label="list" >

        </activity>
         <activity
            android:name="com.swapp.findsenetor.Profile_Page"
            android:label="profile_page" >
