<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.programmingmobile.pageviewer"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LoginView"
            android:label="@string/title_activity_page_view" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
           android:name="com.programmingmobile.pageviewer.SlidePages" class=".SlidePages" android:label="Health Info">
    	</activity>
    	
         <activity
           android:name="com.programmingmobile.pageviewer.PickPages" class=".PickPages" android:label="Choose Health Info">
    	</activity>
    </application>

</manifest>